  padding-right: 20rpx;
}

.auth-group {
  box-sizing: border-box;
  padding: 0 40rpx;
  position: absolute;
  bottom: 20rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-bottom: 54rpx;
}

.auth-refuse {
  color: #7f3a3b;
  font-size: 15px;
  margin-bottom: 10rpx;
}

.auth-refuse-wrap {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-end;
}

.auth-refuse-line {
  width: 144rpx;
  height: 10rpx;
}

.auth-sure {
  width: 302rpx;
  height: 94rpx;
}

.auth-sure-wrap {
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}

.auth-sure-wrap > text {
  position: absolute;
  color: #fff;
  font-size: 15px;
}

.auth-agreement {
  position: absolute;
  color: #b96f1f;
  font-size: 15px;
  width: 416rpx;
  line-height: 60rpx;
  top: 32%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.auth-btn {
  position: absolute;
  height: 94rpx;
  color: #fff;
  width: 100%;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

button[plain] {
  border: 0px solid #353535;
  color: #fff;
}

