/**index.wxss**/
.home-bg {
  position: relative;
}

.activity-bg {
  background: linear-gradient(#7ac73e, #fcde7e);
  height: 1352rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.bg-avatar-wrap {
  position: absolute;
  top: 6rpx;
  left: 16rpx;
}

.bg-avatar {
  display: inline-block;
  width: 132rpx;
  height: 132rpx;
}

.share-box {
  width: 100%;
  position: absolute;
  top: 56%;
  height: 300rpx;
}

.userinfo {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 99;
}

.userinfo-avatar {
  width: 94rpx;
  height: 94rpx;
  margin-top: 24rpx;
  margin-left: 34rpx;
  border-radius: 50%;
}

.peng-title {
  display: flex;
  flex-direction: row;
  align-items: center;
  position: absolute;
  left: 154rpx;
  top: 50rpx;
  width: 222rpx;
  height: 46rpx;
  background: #be7066;
  font-size: 12px;
}

.peng-title-label {
  color: #2d2c2b;
  width: 126rpx;
  height: 38rpx;
  background: #efd2a3;
  border-radius: 2px;
  text-align: center;
  line-height: 38rpx;
  box-shadow: 0px 0px 3px 2px #a15b34;
}

.peng-percent {
  margin-left: 16rpx;
  color: #fff;
  font-size: 15px;
}

.friends-dynamic {
  position: absolute;
  display: inline-block;
  top: 36rpx;
  right: 54rpx;
  width: 166rpx;
  font-size: 12px;
}

.friends-dynamic-title {
  border-top-left-radius: 7px;
  border-top-right-radius: 7px;
  color: #be7066;
  background: #f5d8a5;
  text-align: center;
  height: 40rpx;
  line-height: 40rpx
}

.friends-dynamic-info {
  background: #000;
  opacity: 0.5;
  color: #fff;
  border-bottom-left-radius: 7px;
  border-bottom-right-radius: 7px;
}

.friends-dynamic-info view {
  padding-top: 4rpx;
  padding-bottom: 2rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.navibar {
  position: absolute;
  box-sizing: border-box;
  bottom: 58rpx;
  width: 100%;
  display: flex;
  padding: 0 32rpx;
  flex-direction: row;
  justify-content: space-between;
}

.navibar  image {
  display: inline-block;
  width: 130rpx;
  height: 130rpx;
}

.label-btn {
  background: linear-gradient(#97de38, #5bc222);
  width: 308rpx;
  height: 88rpx;
  text-align: center;
  line-height: 88rpx;
  color: #f9fef5;
  font-size: 17px;
  border-radius: 88rpx;
}

.prize {
  margin-top: -14rpx;
  z-index: 99;
}

.activity-info {
  margin-top: 46rpx;
  margin-bottom: 80rpx;
  border-radius: 14rpx;
  box-shadow: 0px 2px 7px 2px #57952c;
  box-sizing: border-box;
  margin-left: 20px;
  margin-right: 20px;
  width: calc(100% - 40px);
  background: #fcecba;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 16px;
  color: #666;
  padding: 40rpx 30rpx;
}

.activity-info > text {
  line-height: 48rpx;
}

.prize-info-item {
  height: 184rpx;
  flex: 1;
  display: flex;
  font-size: 11px;
  text-align: center;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
  color: #965d07;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
}
.prize-info-item > view {
  margin-left: 10rpx;
}
.prize-info-item > view:nth-child(1) {
  margin-left:0rpx;
}
.prize-image-wrap {
  color: #965d07;
  font-size: 11px;
  width: 134rpx;
  height: 100rpx;
  background: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 16rpx;
  border: 1px solid #f3c96a;
  border-radius: 3px;
}

.prize-info-item > view {
  width: 25%;
}

.prize-image-wrap  image {
  width: 108rpx;
  height: 50rpx;
}

.left-arrow, .right-arrow {
  width: 14rpx;
  height: 24rpx;
  margin: 0 14rpx;
  margin-top: -40rpx;
}

.prize-info {
  height: 186rpx;
  padding-left: 0;
  padding-right: 0;
}

.rule-info {
}

.others {
  padding: 0;
  background: transparent;
  box-shadow: 0px 2px 7px 2px transparent;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.others image {
  width: 314rpx;
  height: 266rpx;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}

.download-lujinsuo {
  width: 309rpx;
  height: 102rpx;
  position: relative;
  text-align: center;
}

.download-lujinsuo-text {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  font-size: 17px;
  color: #fff;
  width: 100%;
}

.download-software {
  background: #61bd21;
  padding: 34rpx 40rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  height: 164rpx;
}

.mask-box {
  width: 100%;
  height: 100%;
  background: #040404;
  opacity: 0.6;
  position: absolute;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  top: 0;
  left: 0;
  z-index: 99;
}

.pop-win-bg {
  width: 690rpx;
  height: 668rpx;
}

.pop-win-record-bg {
  width: 690rpx;
  height: 1048rpx;
}

.scratch-wrap {
  position: absolute;
  top: 48%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.pop-win {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 100;
}

.show {
  display: block;
}

.hide {
  display: none;
}

.scratch-title > image {
  width: 300rpx;
  height: 74rpx;
}

.close-btn {
  text-align: right;
  bottom: 10px;
  width: 100%;
  top: -22rpx;
  right: 20rpx;
