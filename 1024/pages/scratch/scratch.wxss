
.mask-box{
  width: 100%;
  height: 100%;
  background:#040404;
  opacity: 0.6;
  position: absolute;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  top:0;
  left: 0;
}
.pop-win-bg{
  width: 690rpx;
  height: 668rpx;
}
.scratch-wrap{
  position: absolute;
  top: 44%;
  left: 50%;
  transform:translate(-50%,-50%);
}
.pop-win{
   position: absolute;
   top: 50%;
   left: 50%;
   transform:translate(-50%,-50%);
}